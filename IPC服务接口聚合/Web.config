<?xml version="1.0"?>
<!-- 
    注意: 除了手动编辑此文件，还可用 
    Web 管理工具配置应用程序设置。可使用
    Visual Studio 中的“网站”->“Asp.Net 配置”选项。
    所有设置和注释都列在 
    machine.config.comments 中，该文件通常位于 
    \Windows\Microsoft.Net\Framework\vx.x\Config 中
-->
<configuration>

  <connectionStrings>
    <!-- 数据库连接 -->
    <add name="mainsqlserver" connectionString="Data Source=db.db.com;Initial Catalog=gzf;Persist Security Info=True;User ID=sa;Password=100zzcom;Min Pool Size=100;Max Pool Size=5000;Connection Lifetime=50;Connect Timeout=60;Workstation ID=ipc" providerName="System.Data.SqlClient"/>
    <!-- IPC接口聚合中心配置 , IPCurl是接口中心地址，ThisAppPoolName是本地应用进程池名（部署时清理进程用的） -->
    <add name="IPCurl" connectionString="http://ipc.gzf.com/fmwscenter.asmx"/>
    <add name="ThisAppPoolName" connectionString="公租房-IPC服务接口聚合"/>
  </connectionStrings>
  <appSettings>
    <!-- 调用方识别标记配置 -->
    <add key="DYFSBBJ" value=",公租房-ws界面中心,公租房-ws核心业务中心" />
 
  </appSettings>
  <system.web>
    <compilation targetFramework="4.0" debug="true"/>
  </system.web>
  <runtime>
    <!-- 为应用程序禁用 CAS 发行者策略检查 -->
    <generatePublisherEvidence enabled="false"/>
  </runtime>
</configuration>